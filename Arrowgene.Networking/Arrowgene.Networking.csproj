    <TargetFramework>netstandard2.1</TargetFramework>
    <RootNamespace>Arrowgene.Networking</RootNamespace>
    <Title>Arrowgene.Networking</Title>
    <Company>Arrowgene</Company>
    <Copyright>Copyright © 2017-2020 Arrowgene</Copyright>
    <Version>$(Version)</Version>
    <Authors>sebastian-heinz</Authors>
    <Description>Server socket</Description>
